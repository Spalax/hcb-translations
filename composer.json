{
    "name": "huskycms/hcb-translations",
    "type": "huskycms-backend-plugin",
    "description": "Managing translations plugin for backend area of the HuskyCMS",
    "authors": [
        {
            "name": "Mylotskyi Oleksii",
            "email": "spalaxinco@gmail.com"
        }
    ],
    "keywords": [
        "huskycms", "plugin", "module"
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Spalax/zf2-libs.git"
        },
        {
            "type": "composer",
            "url": "http://spalax:spalax@packages.milsdev.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.3.3",
        "huskycms/hc-backend": "dev-master",
        "spalax/msgfmt": "dev-master",
        "spalax/zf2-libs": "dev-master",
        "zendframework/zendframework": "2.2.0",
        "doctrine/data-fixtures":  "v1.0.0-ALPHA3",
        "doctrine/doctrine-module": "0.*",
        "doctrine/doctrine-orm-module": "0.*",
        "doctrine/migrations": "v1.0-ALPHA1",
        "zetacomponents/archive": "dev-master",
        "rwoverdijk/assetmanager": "1.3.5"
    },
    "autoload": {
        "psr-0": {
            "HcbTranslations": "src/"
        },

        "classmap": [
            "./Module.php"
        ]
    },
    "minimum-stability": "dev"
}
